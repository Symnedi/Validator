{
	"name": "symnedi/validator",
	"description": "Symfony\\Validator integration to Nette.",
	"keywords": ["nette", "symfony", "validator"],
	"license": "MIT",
	"authors": [
		{ "name": "Tomáš Votruba", "homepage": "http://tomasvotruba.cz", "email": "tomas.vot@gmail.com" }
	],
	"require": {
		"php": ">=5.6",
		"kdyby/annotations": "~2.2.0",
		"nette/caching": "~2.3.0|~2.4.0",
		"nette/di": "~2.3.0",
		"symfony/validator": "~2.7.0"
	},
	"require-dev": {
		"nette/bootstrap": "~2.3.0"
	},
	"autoload": {
		"psr-4": {
			"Symnedi\\Validator\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Symnedi\\Validator\\Tests\\": "tests"
		}
	}
